@model IEnumerable<skjatextar.Models.Translation>
@{
    ViewBag.Title = "ViewVideo";
}

<div class="table-header-home">
    <span>Nýjustu 10</span>
</div>
<div class="content-home">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th class="translations">Allir textar fyrir ákveðna mynd</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>>@item.Title</td>
                </tr>
            }
        </tbody>
    </table>
</div>
